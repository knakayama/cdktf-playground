{
  "name": "cdktf-play-groud",
  "packageManager": "yarn@3.2.2",
  "version": "0.0.1",
  "description": "CDKTF play ground",
  "scripts": {
    "lint:ec": "ec --exclude 'node_modules|dist|\\.git|cdk\\.out|\\.yarn'",
    "lint:eslint": "eslint --ext .ts src",
    "lint:prettier": "prettier --check 'packages/**/*.{ts,yaml,md,json}' '*.{md,yaml,json,js,ts}'",
    "lint": "yarn lint:ec && yarn lint:prettier && yarn lint:eslint",
    "postinstall": "husky install",
    "build": "tsc",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "yarn workspace"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18.7.0",
    "npm": "please_use_yarn_instead"
  },
  "license": "MIT",
  "repository": "https://github.com/knakayama/cdktf-play-ground.git",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cdktf-cli": "^0.12.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@cdktf/provider-aws": "9.0.10",
    "@cdktf/provider-docker": "2.0.9",
    "cdktf": "^0.12.0",
    "constructs": "^10.1.65"
  }
}
